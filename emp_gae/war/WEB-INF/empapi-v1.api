{
  "extends" : "thirdParty.api",
  "abstract" : false,
  "root" : "https://jhcremp.appspot.com/_ah/api",
  "name" : "empapi",
  "version" : "v1",
  "description" : "Employee API",
  "defaultVersion" : false,
  "adapter" : {
    "bns" : "http://jhcremp.appspot.com/_ah/spi",
    "deadline" : 10.0,
    "type" : "lily"
  },
  "auth" : {
    "allowCookieAuth" : false
  },
  "frontendLimits" : {
    "unregisteredUserQps" : -1,
    "unregisteredQps" : -1,
    "unregisteredDaily" : -1,
    "rules" : [ ]
  },
  "cacheControl" : {
    "type" : "no-cache",
    "maxAge" : 0
  },
  "methods" : {
    "empapi.save" : {
      "path" : "employees",
      "httpMethod" : "POST",
      "scopes" : [ ],
      "audiences" : [ ],
      "clientIds" : [ ],
      "rosyMethod" : "EmpEndpoint.save",
      "request" : {
        "body" : "autoTemplate(backendRequest)",
        "bodyName" : "resource"
      },
      "response" : {
        "body" : "empty"
      }
    },
    "empapi.get" : {
      "path" : "employees/{id}",
      "httpMethod" : "GET",
      "scopes" : [ ],
      "audiences" : [ ],
      "clientIds" : [ ],
      "rosyMethod" : "EmpEndpoint.get",
      "request" : {
        "parameters" : {
          "id" : {
            "type" : "string",
            "required" : true
          }
        },
        "body" : "empty"
      },
      "response" : {
        "body" : "autoTemplate(backendResponse)"
      }
    },
    "empapi.delete" : {
      "path" : "employees/{id}",
      "httpMethod" : "DELETE",
      "scopes" : [ ],
      "audiences" : [ ],
      "clientIds" : [ ],
      "rosyMethod" : "EmpEndpoint.delete",
      "request" : {
        "parameters" : {
          "id" : {
            "type" : "string",
            "required" : true
          }
        },
        "body" : "empty"
      },
      "response" : {
        "body" : "empty"
      }
    },
    "empapi.list" : {
      "path" : "employees",
      "httpMethod" : "GET",
      "scopes" : [ ],
      "audiences" : [ ],
      "clientIds" : [ ],
      "rosyMethod" : "EmpEndpoint.list",
      "request" : {
        "body" : "empty"
      },
      "response" : {
        "body" : "autoTemplate(backendResponse)"
      }
    }
  },
  "descriptor" : {
    "schemas" : {
      "Employee" : {
        "id" : "Employee",
        "type" : "object",
        "properties" : {
          "firstName" : {
            "type" : "string"
          },
          "id" : {
            "type" : "string"
          },
          "lastName" : {
            "type" : "string"
          },
          "phones" : {
            "type" : "array",
            "items" : {
              "$ref" : "Phone"
            }
          }
        }
      },
      "Phone" : {
        "id" : "Phone",
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string"
          },
          "number" : {
            "type" : "string"
          }
        }
      },
      "EmployeeCollection" : {
        "id" : "EmployeeCollection",
        "type" : "object",
        "properties" : {
          "items" : {
            "type" : "array",
            "items" : {
              "$ref" : "Employee"
            }
          }
        }
      }
    },
    "methods" : {
      "EmpEndpoint.save" : {
        "request" : {
          "$ref" : "Employee"
        }
      },
      "EmpEndpoint.get" : {
        "response" : {
          "$ref" : "Employee"
        }
      },
      "EmpEndpoint.delete" : {
      },
      "EmpEndpoint.list" : {
        "response" : {
          "$ref" : "EmployeeCollection"
        }
      }
    }
  }
}